package java01;


public class java011 {

	public static void main(String[] args) {
		System.out.println("furkan");
		Car c1 = new Car(4, 0);
		
		System.out.println(c1.model);
	}

}

class CarDeneme {
	int tekerlekSayisi;
	int model;
	
	public Car(int tekerlekSayisi, int model) {
		this.tekerlekSayisi = tekerlekSayisi;
		this.model = model;
	}
}